{
	"name": "portiny/graphql",
	"description": "Lightweight GraphQL integration extension for Nette framework.",
	"license": ["MIT"],
	"require": {
		"php": "~7.1",
		"nette/http": "^2.4",
		"nette/utils": "^2.4",
		"tracy/tracy": "^2.4",
		"webonyx/graphql-php": "^0.11"
	},
	"require-dev": {
		"nette/di": "^2.4",
		"nette/bootstrap": "^2.4",
		"phpunit/phpunit": "^6.5"
	},
	"autoload": {
		"psr-4": {
			"Portiny\\GraphQL\\": "src"
		}
	},
	"autoload-dev": {
